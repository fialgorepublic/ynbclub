<div class="col-md-9">
  <div class="row">
    <div class="col-md-12">
      <div class="main">
        <h2> <i class="fa fa-money"></i> <%= t('sidebar.earnings_label') %></h2>
      </div>
      <hr>
    </div>
  </div>

  <div class="row">
    <div class="col-md-12">
      <h3><%= t(:last_earned_coins_label) %></h3>
    </div>
  </div>

  <div class="row">
    <%= render partial: 'dashboard/point', collection: @points %>
  </div>

  <% if current_user_points(current_user) > 4 %>
    <div class="row">
      <div class="col-md-offset-10 col-md-2">
        <a href="/users/points"><%= t(:show_more_label) %>...</a>
      </div>
    </div>
  <% end %>
</div>
