<div class="dashboard-holder">
  <div class="row">
    <div class="col-md-3">
      <%= render partial: "shared/side_bar" %>
    </div>
    <div class="col-md-9">

      <div class="row">
        <div class="col-md-12">
          <div class="main">
            <h2> <i class="fa fa-share-alt fa-lg fa-fw sidebar-icon"></i>Take a snapshot and Share</h2>
          </div>
          <hr>
        </div>
      </div>

      <div class="row">
        <div class="col-md-12">
          <div class="box-shadow">
            <div class="row">
              <div class="col-md-12">
                <p class="text-center">Insert copied link to your selfie or video. Post must contain <strong>hastag #saintL'beau</strong></p>
                <div class="col-md-12">
                  <div class="form-box" style="overflow: hidden">
                    <span>Link to facebook, instagram, twitter post </span>

                  </div>
                </div>
                <div class="col-md-12 ">
                  <a href="#" target="_blank" class="btn btn-primary btn-lg btn-block">Continue</a>
                </div>

              </div>
            </div>






          </div>
        </div>
      </div>


    </div>
  </div>
</div>

