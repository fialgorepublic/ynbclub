    <!-- Modal content-->
    <div class="modal-content con-pos-rel">
      <button type="button" class="close btn-close-con" data-dismiss="modal">&times;</button>

      <div class="modal-body">
        <div class="row">
          <p class="color-555">preview</p>
          <div class="col-md-4 b-con">

