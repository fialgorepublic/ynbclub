var conversationID = <%= params[:id] %>;
var linkElement = $(`[data-conversation-id=${conversationID}]`)
linkElement.attr('href', '<%= like_conversation_path(params[:id]) %>')
linkElement.find('i').addClass('green-color');
linkElement.find('i').removeClass('red-color');
var countEle = $(`.likes-count-${conversationID}`);
count    = $(countEle[0]).text();
newCount =  parseInt(count) - 1;
countEle.text(newCount)
